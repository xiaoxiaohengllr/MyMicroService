{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/MyMicroService.Service.WeChatAppletService",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:49586",
      "sslPort": 0
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "Executable",
      "commandLineArgs": "--urls http://localhost:5002 --IP localhost --Port 5002 --ConsulUrl http://localhost:8500/",
      "launchBrowser": true,
      "launchUrl": "weatherforecast",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "MyMicroService.Service.WeChatAppletService": {
      "commandName": "Project",
      "commandLineArgs": "--urls http://localhost:5002 --IP localhost --Port 5002 --ConsulUrl http://localhost:8500/",
      "launchBrowser": true,
      "launchUrl": "weatherforecast",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/weatherforecast",
      "publishAllPorts": true
    }
  }
}